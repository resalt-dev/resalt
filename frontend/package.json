{
	"name": "svelte-app",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 5555 --strictPort --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check src && eslint src",
		"eslint": "eslint src",
		"format": "prettier --plugin-search-dir . --write src",
		"start": "sirv build --no-clear --single --port 5555 --host 0.0.0.0"
	},
	"devDependencies": {
		"@codemirror/lang-json": "^6.0.0",
		"@codemirror/language": "^6.2.1",
		"@codemirror/state": "^6.1.1",
		"@codemirror/view": "^6.2.3",
		"@eonasdan/tempus-dominus": "^6.2.10",
		"@lezer/highlight": "^1.0.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/vite-plugin-svelte": "^2.0.0",
		"@tsconfig/svelte": "^3.0.0",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"codemirror": "^6.0.1",
		"eslint": "^8.30.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.55.0",
		"sirv-cli": "^2.0.2",
		"svelte": "^3.55.0",
		"svelte-chartjs": "^3.1.0",
		"chart.js": "^4.1.1",
		"svelte-check": "^2.10.0",
		"svelte-local-storage-store": "^0.3.1",
		"svelte-navigator": "^3.2.2",
		"svelte-preprocess": "^4.10.7",
		"sveltestrap": "^5.9.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"uuid": "^9.0.0",
		"vite": "^4.0.0"
	}
