		"@lezer/highlight": "^1.1.6",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/vite-plugin-svelte": "^2.4.1",
		"@tsconfig/svelte": "^4.0.1",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
