{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "camelcase": "error",
        "indent": [
            "error",
            4
        ],
        "no-console": "off",
        "import/no-extraneous-dependencies": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-param-reassign": "off",
        "implicit-arrow-linebreak": "off",
        "function-paren-newline": "off"
    }
}
