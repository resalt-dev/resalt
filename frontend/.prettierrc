{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"tabWidth": 4,
	"semi": true,
	"arrowParens": "always",
	"bracketSpacing": true,
	"proseWrap": "preserve",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
